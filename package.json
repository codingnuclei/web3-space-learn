{
  "name": "web3",
  "version": "0.0.1",
  "license": "MIT",
  "scripts": {
    "ng": "nx",
    "postinstall": "node ./decorate-angular-cli.js && ngcc --properties es2015 browser module main",
    "config:local": "cd ./apps/chain-chat && ts-node -r dotenv/config  ./setEnv.ts dotenv_config_path=./blockchain/local.env",
    "config:ropsten": "cd ./apps/chain-chat && ts-node -r dotenv/config  ./setEnv.ts dotenv_config_path=./blockchain/ropsten.env",
    "start": "nx serve",
    "build": "nx build",
    "test": "nx test",
    "fork": "cd apps/chain-chat/blockchain && hardhat node",
    "compile": "cd apps/chain-chat/blockchain && hardhat compile",
    "deploy:local": "cd apps/chain-chat/blockchain && NETWORK=local hardhat --network localhost deploy",
    "deploy:ropsten": "cd apps/chain-chat/blockchain && NETWORK=ropsten hardhat --network ropsten deploy",
    "local:clean": "cd apps/chain-chat/blockchain && hardhat clean"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~13.1.0",
    "@angular/common": "~13.1.0",
    "@angular/compiler": "~13.1.0",
    "@angular/core": "~13.1.0",
    "@angular/forms": "~13.1.0",
    "@angular/platform-browser": "~13.1.0",
    "@angular/platform-browser-dynamic": "~13.1.0",
    "@angular/router": "~13.1.0",
    "@ngneat/hot-toast": "^4.1.0",
    "@ngneat/overview": "^3.0.0",
    "@rx-angular/cdk": "latest",
    "@rx-angular/template": "^1.0.0-beta.29",
    "hardhat-deploy": "^0.9.24",
    "rxjs": "~7.4.0",
    "tslib": "^2.0.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~13.1.0",
    "@angular-eslint/eslint-plugin": "~13.0.1",
    "@angular-eslint/eslint-plugin-template": "~13.0.1",
    "@angular-eslint/template-parser": "~13.0.1",
    "@angular/cli": "~13.1.0",
    "@angular/compiler-cli": "~13.1.0",
    "@angular/language-service": "~13.1.0",
    "@nomiclabs/hardhat-ethers": "^2.0.3",
    "@nomiclabs/hardhat-etherscan": "^2.1.8",
    "@nomiclabs/hardhat-waffle": "^2.0.1",
    "@nrwl/angular": "13.4.3",
    "@nrwl/cli": "13.4.3",
    "@nrwl/cypress": "13.4.3",
    "@nrwl/eslint-plugin-nx": "13.4.3",
    "@nrwl/jest": "13.4.3",
    "@nrwl/linter": "13.4.3",
    "@nrwl/tao": "13.4.3",
    "@nrwl/workspace": "13.4.3",
    "@typechain/ethers-v5": "^7.2.0",
    "@typechain/hardhat": "^2.3.1",
    "@types/jest": "27.0.2",
    "@types/node": "14.14.33",
    "@typescript-eslint/eslint-plugin": "~5.3.0",
    "@typescript-eslint/parser": "~5.3.0",
    "chai": "^4.3.4",
    "cypress": "^9.1.0",
    "eslint": "8.2.0",
    "eslint-config-prettier": "8.1.0",
    "eslint-plugin-cypress": "^2.10.3",
    "ethereum-waffle": "^3.4.0",
    "ethers": "^5.5.2",
    "hardhat": "^2.8.0",
    "hardhat-gas-reporter": "^1.0.6",
    "jest": "27.2.3",
    "jest-preset-angular": "11.0.0",
    "prettier": "^2.5.1",
    "prettier-plugin-organize-attributes": "0.0.4",
